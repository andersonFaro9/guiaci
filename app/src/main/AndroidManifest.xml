<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.guiacistore">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACTION_DIAL" />

    <application
        android:name=".CustomAplication"
        android:allowBackup="true"
        android:icon="@mipmap/guiaci"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".splash.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/theme_splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".lojas.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".lojas.SpeedNetActivity"
            android:parentActivityName=".lojas.SpeedNetActivity"
            android:theme="@style/themeSpeednet" />
        <activity
            android:name=".lojas.SobreActivity"
            android:theme="@style/barra_sobre" />
        <activity android:name=".StorageActivity" />
        <activity
            android:name=".lojas.BiosInformaticaActivity"
            android:parentActivityName=".lojas.BiosInformaticaActivity"
            android:theme="@style/themeBios"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".lojas.QuadrangularActivity"
            android:parentActivityName=".lojas.QuadrangularActivity"
            android:theme="@style/themeQuadrangular" />
        <activity
            android:name=".lojas.GregBarbeariaActivity"
            android:parentActivityName=".lojas.GregBarbeariaActivity"
            android:theme="@style/themeGreg" />

        <activity
            android:name=".lojas.JobSatActivity"
            android:parentActivityName=".lojas.JobSatActivity"
            android:theme="@style/themeJobSat" />

        <activity android:name=".lojas.RepSolActivity"
            android:parentActivityName=".lojas.RepSolActivity"
            android:theme="@style/colorPrimaryRepSol" />

        <activity android:name=".lojas.LiliDocesCakesActivity"
            android:parentActivityName=".lojas.LiliDocesCakesActivity"
            android:theme="@style/themeDocesCake" />

        <activity android:name=".lojas.AcademiaEvolutionActivity"
            android:parentActivityName=".lojas.AcademiaEvolutionActivity"
            android:theme="@style/themeEvolution"
            />


        <activity android:theme="@style/themeMaseratt"
            android:parentActivityName=".lojas.StudioMaserattActivity"
            android:name=".lojas.StudioMaserattActivity" />


        <activity android:name=".lojas.StudioESoundActivity" android:theme="@style/themeEsound"
            android:parentActivityName=".lojas.StudioESoundActivity"/>

        <activity android:name=".lojas.RosaDeSaronActivity"  android:theme="@style/colorPrimaryRosaDeSaron" android:parentActivityName=".lojas.RosaDeSaronActivity"/>

        <activity android:name=".lojas.InoveActivity"  android:parentActivityName=".lojas.InoveActivity" android:theme="@style/themeInove" />
        <activity android:name=".lojas.EpgActivity" android:theme="@style/themeEpg" android:parentActivityName=".lojas.EpgActivity" />


        <activity android:screenOrientation="portrait" android:name=".redesocial.SpeednetRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.GregBarbeariaRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.IgrejaQuadrangularRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.JobSatRedesSociaisActivity"/>
        <activity android:screenOrientation="portrait" android:name=".redesocial.LiliDocesCakesRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.AcademiaEvolutionRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.RepsolRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.BiosInformaticaRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.IgrejaRosaDeSaronRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.StudioMaserattActivityRedesSociaisActivity" />
        <activity android:screenOrientation="portrait" android:name=".redesocial.InoveRedesSociaisActivity" />

        <activity android:name=".redesocial.EpgContatosRedesSociaisActivity" />
        <activity android:name=".lojas.BlueWayActivity" />


    </application>

</manifest>